
public class SumMatrix {
	static int[][] sumMatrix(int[][] A, int[][] B){
		int[][] answer = {{0,0}, {0,0}};
		for(int i=0; i<2; i++) {
			for(int k=0; k<2; k++) {
				answer[i][k] = A[i][k] + B[i][k];
			}
		}
		
		return answer;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] A = {{1,2}, 
					{2,3}};
		
		int[][] B = {{3,4}, 
					{5,6}};
		
		int[][] answer = sumMatrix(A,B);
		System.out.println(answer);
	}

}
